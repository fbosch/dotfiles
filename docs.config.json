{
  "$schema": "https://raw.githubusercontent.com/fbosch/docs-cache/main/docs.config.schema.json",
  "sources": [
    {
      "include": [
        "src/app/extensions/**/*.mdx"
      ],
      "id": "vicinae-docs",
      "repo": "https://github.com/vicinaehq/docs.git",
      "targetDir": "./.config/vicinae/extensions/docs/agents/references"
    },
    {
      "include": [
        "docs/guide/**/*.md"
      ],
      "id": "ags",
      "repo": "https://github.com/Aylur/ags.git",
      "targetDir": "./.config/ags/docs/agents/guide"
    },
    {
      "include": [
        "content/Configuring/**/*.md"
      ],
      "id": "hyprland-wiki",
      "repo": "https://github.com/hyprwm/hyprland-wiki.git",
      "targetDir": "./.config/hypr/docs/agents/references"
    },
    {
      "include": [
        "Configuration.md",
        "Styling.md"
      ],
      "id": "waybar",
      "repo": "https://github.com/Alexays/Waybar.wiki.git"
    },
    {
      "id": "glance",
      "repo": "https://github.com/glanceapp/glance",
      "include": [
        "docs/**/*.md"
      ],
      "targetDir": "./.config/glance/docs/agents/references"
    }
  ]
}
