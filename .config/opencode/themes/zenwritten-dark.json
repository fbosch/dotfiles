{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "darkBg": "#191919",
    "darkBg2": "#222222",
    "darkBg3": "#2C2C2C",
    "darkFg": "#BBBBBB",
    "darkMuted": "#686868",
    "darkSubtle": "#555555",
    "darkBorder": "#616161",
    "lightBg": "#EEEEEE",
    "lightBg2": "#E5E5E5", 
    "lightBg3": "#D7D7D7",
    "lightFg": "#353535",
    "lightMuted": "#8B8B8B",
    "red": "#DE6E7C",
    "orange": "#B77E64",
    "green": "#819B69",
    "blue": "#6099C0",
    "cyan": "#66A5AD",
    "purple": "#B279A7",
    "gray": "#8E8E8E"
  },
  "theme": {
    "primary": {
      "dark": "cyan",
      "light": "blue"
    },
    "secondary": {
      "dark": "blue",
      "light": "cyan"
    },
    "accent": {
      "dark": "purple",
      "light": "purple"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "orange",
      "light": "orange"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "blue",
      "light": "blue"
    },
    "text": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "textMuted": {
      "dark": "darkMuted",
      "light": "lightMuted"
    },
    "background": {
      "dark": "darkBg",
      "light": "lightBg"
    },
    "backgroundPanel": {
      "dark": "darkBg3",
      "light": "lightBg3"
    },
    "backgroundElement": {
      "dark": "darkBg2",
      "light": "lightBg2"
    },
    "border": {
      "dark": "darkBorder",
      "light": "darkBorder"
    },
    "borderActive": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "borderSubtle": {
      "dark": "darkSubtle",
      "light": "darkSubtle"
    },
    "diffAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "darkMuted",
      "light": "lightMuted"
    },
    "diffHunkHeader": {
      "dark": "purple",
      "light": "purple"
    },
    "diffHighlightAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffHighlightRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffAddedBg": {
      "dark": "#232D1A",
      "light": "#CBE5B8"
    },
    "diffRemovedBg": {
      "dark": "#3E2225",
      "light": "#EBD8DA"
    },
    "diffContextBg": {
      "dark": "darkBg",
      "light": "lightBg"
    },
    "diffLineNumber": {
      "dark": "darkBorder",
      "light": "darkBorder"
    },
    "diffAddedLineNumberBg": {
      "dark": "#232D1A",
      "light": "#CBE5B8"
    },
    "diffRemovedLineNumberBg": {
      "dark": "#3E2225",
      "light": "#EBD8DA"
    },
    "markdownText": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "markdownHeading": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "markdownLink": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownLinkText": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownCode": {
      "dark": "gray",
      "light": "gray"
    },
    "markdownBlockQuote": {
      "dark": "darkMuted",
      "light": "lightMuted"
    },
    "markdownEmph": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "markdownStrong": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "markdownHorizontalRule": {
      "dark": "darkBorder",
      "light": "darkBorder"
    },
    "markdownListItem": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "markdownListEnumeration": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "markdownImage": {
      "dark": "purple",
      "light": "purple"
    },
    "markdownImageText": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownCodeBlock": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "syntaxComment": {
      "dark": "darkMuted",
      "light": "lightMuted"
    },
    "syntaxKeyword": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "syntaxFunction": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "syntaxVariable": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "syntaxString": {
      "dark": "gray",
      "light": "gray"
    },
    "syntaxNumber": {
      "dark": "gray",
      "light": "gray"
    },
    "syntaxType": {
      "dark": "gray",
      "light": "#735057"
    },
    "syntaxOperator": {
      "dark": "darkFg",
      "light": "lightFg"
    },
    "syntaxPunctuation": {
      "dark": "darkFg",
      "light": "lightFg"
    }
  }
}
