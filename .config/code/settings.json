{
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.codeLens": true,
  "editor.fontFamily": "Zenbones Brainy, JetBrains Mono, Symbols Nerd Font Mono",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "relative",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "editor.bracketPairColorization.enabled": false,
  "workbench.colorCustomizations": {
    "breadcrumb.background": "#191919",
    "breadcrumb.foreground": "#bbbbbb",
    "breadcrumb.focusForeground": "#ffffff",
    "breadcrumb.activeSelectionForeground": "#ffffff",
    "breadcrumbPicker.background": "#191919",
    "breadcrumb.shadow": "#00000000",
    "window.activeBorder": "#19191900",
    "window.inactiveBorder": "#19191900",
    "editorGroupHeader.tabsBackground": "#191919",
    "editorGroupHeader.border": "#191919",
    "editor.background": "#191919",
    "gitDecoration.modifiedResourceForeground": "#D68C67",
    "sideBar.background": "#191919",
    "sideBar.border": "#303030",
    "sideBarSectionHeader.background": "#191919",
    "sideBarSectionHeader.foreground": "#bbbbbb",
    "statusBar.background": "#222",
    "statusBar.border": "#2c2c2c",
    "statusBar.foreground": "#bbbbbb",
    "titleBar.activeBackground": "#191919",
    "tab.activeBackground": "#222",
    "tab.activeBorderTop": "#303030",
    "tab.activeForeground": "#ffffff",
    "tab.activeModifiedBorder": "#D68C67",
    "tab.border": "#222",
    "tab.hoverBackground": "#252525",
    "tab.inactiveBackground": "#131313",
    "terminal.ansiBlack": "#131313",
    "terminal.ansiYellow": "#E5B769"
  },
  "zenMode.centerLayout": false,
  "zenMode.fullScreen": false,
  "zenMode.hideStatusBar": false,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.experimental.useTsgo": true,
  "explorer.confirmDragAndDrop": false,
  "editor.lineHeight": 1.5,
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.enablePreview": true,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.colorTheme": "Zenbones Zenwritten Dark",
  "terminal.integrated.fontFamily": "Zenbones Brainy, JetBrains Mono, Symbols Nerd Font Mono, BabelStone Runic Elder Futhark",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontLigatures": true,
  "workbench.sideBar.location": "right",
  "editor.stickyScroll.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "workbench.editor.highlightModifiedTabs": true,
  "typescript.tsserver.maxTsServerMemory": 4096,
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.showFoldingControls": "never",
  "workbench.layoutControl.type": "menu",
  "window.title": " ",
  "window.titleBarStyle": "custom",
  "window.customTitleBarVisibility": "never",
  "explorer.autoReveal": "focusNoScroll",
  "search.ripgrep.maxThreads": 16,
  "search.quickOpen.includeSymbols": true,
  "search.quickAccess.preserveInput": true,
  "accessibility.dimUnfocused.enabled": true,
  "accessibility.dimUnfocused.opacity": 0.6,
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "window.density.editorTabHeight": "compact",
  "workbench.editor.pinnedTabSizing": "compact",
  "breadcrumbs.filePath": "last",
  "breadcrumbs.showArrays": false,
  "breadcrumbs.symbolPath": "off",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.navigationControl.enabled": false,
  "workbench.activityBar.location": "hidden",
  "workbench.activityBar.orientation": "vertical",
  "colorize.decoration_type": "square-dot",
  "colorize.include": [
    "**/*.lua",
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "**/*.styl"
  ],
  "workbench.layoutControl.enabled": false,
  "cursor.composer.textSizeScale": 1.15,
  "terminal.integrated.altClickMovesCursor": false,
  "terminal.integrated.defaultProfile.osx": "dash",
  "terminal.integrated.env.osx": {
    "INSIDE_CURSOR": "1"
  },
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "args": ["-l"],
      "icon": "terminal-bash"
    },
    "dash": {
      "path": "dash",
      "args": ["-l"]
    },
    "fish": {
      "path": "fish",
      "args": ["-l"]
    }
  },
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.defaultProfile.linux": "bash",
  "breadcrumbs.enabled": false,
  "symbols.hidesExplorerArrows": false,
  "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-dark",
  "git.autofetch": true,
  "terminal.integrated.ignoreProcessNames": [
    "starship",
    "oh-my-posh",
    "zsh",
    "fish"
  ],
  "periscope.rgPath": "/usr/local/bin/rg",
  "periscope.rgOptions": [
    "--max-columns=200",
    "--smart-case",
    "--no-heading",
    "--threads=8",
    "--max-count=1000",
    "--max-filesize=1M"
  ],
  "periscope.rgGlobExcludes": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.yarn/**",
    "**/dist/**",
    "**/.pnpm-store/**",
    "**/.backup/**",
    "**/.sessions/**",
    "**/.undo/**",
    "**/.DS_Store",
    "**/.eslintcache"
  ],
  "periscope.showWorkspaceFolderInFilePath": false,
  "periscope.startFolderDisplayDepth": 2,
  "periscope.endFolderDisplayDepth": 2,
  "explorer.excludeGitIgnore": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "clock.dateFormat": "HH:MM",
  "git.openRepositoryInParentFolders": "never"
}
