{
  "$schema": "https://raw.githubusercontent.com/vicinaehq/vicinae/refs/heads/main/extra/schemas/extension.json",
  "name": "nerdfont-search",
  "title": "NerdFonts",
  "description": "utility to search and find nerdfont icons and add them to your clipboard",
  "categories": [],
  "license": "MIT",
  "author": "fbosch",
  "contributors": [],
  "pastContributors": [],
  "icon": "extension_icon.png",
  "commands": [
