{
  "name": "vicinae-extensions",
  "version": "1.0.0",
  "private": true,
  "description": "Vicinae extension workspace",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "clean": "pnpm -r exec rm -rf dist node_modules"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tanstack/react-query": "^5.90.17",
    "@vicinae/api": "^0.18.3",
    "typescript": "^5.9.3"
  },
  "docs-cache": {
    "$schema": "https://raw.githubusercontent.com/fbosch/docs-cache/main/docs.config.schema.json",
    "sources": [
      {
        "id": "vicinae-docs",
        "repo": "https://github.com/vicinaehq/docs.git",
        "targetDir": "./docs/agents/docs"
      }
    ]
  }
}
